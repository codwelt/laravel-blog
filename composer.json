{
    "name": "codwelt/laravel-blog",
    "description": "Blog para laravel",
    "require": {
        "php": ">= 7.1",
        "ext-dom": "*",
        "ext-gmp": "*",
        "ext-bcmath": "*",
        "ext-gd":  "*",
        "laravel/framework": "~5.6 | ~6.0",
        "laravel/scout": "~4.0 | ~5.0 | ~6.0 | ~7.0",
        "yab/laravel-scout-mysql-driver": "^2.0",
        "sebastian/diff": "^3.0",
        "vinkla/hashids": "~4.0 | ~5.0 | ~6.0 | ~7.0",
        "bitpress/blade-extensions": "^1.0",
        "artesaos/seotools": "~0.16",
        "spatie/laravel-robots-middleware": "^1.0",
        "justinrainbow/json-schema": "~5.2",
        "intervention/image": "^2.4",
        "codwelt/helpersman": "~2.1",
        "laravelium/sitemap": "~3.0 | ~4.0 | ~5.0 | ~6.0"
    },
    "authors": [
        {
            "name": "FuriosoJack",
            "email": "furiosojuan0@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "Codwelt\\Blog\\": "src"
        },
        "files":[
            "src/Helpers/Basic.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Codwelt\\Blog\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require-dev": {
        "symfony/thanks": "^1.0",
        "doctrine/dbal": "^2.8",
        "orchestra/testbench": "~3.6 | ~4.0 ",
        "orchestra/testbench-browser-kit": "~3.6 | ~4.0",
        "ext-pdo_sqlite": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codwelt\\Blog\\BlogServiceProvider"
            ]
        }
    }
}
